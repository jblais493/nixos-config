`(format-time-string "%Y/%m/%d")` * ${1:Payee}
    ; ${2:Description}
    ${3:Expenses:${4:Category}}                      ${5:CAD} ${6:Amount}
    ${7:Assets:${8:Bank:Checking}}                   ${9:CAD} ${10:Amount}
$0
